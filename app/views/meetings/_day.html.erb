<div class='item'>
        <div class='date'>
          <%= day[:month] %>/<%= day[:date] %><%=day[:wday]%>
        </div>
        <ul class='content'>
        <% if day[:plans].length != 0 %>
          <% day[:plans].each do |plan| %>
            <li >
            <div class = "plan-list">
            <%= plan[:time] %>
            <%= link_to @temporarys.where(id: plan[:temporary_id])[0].full_name , temporary_path(plan[:temporary_id]) , class: "temporary-show-path"%>
            <%= link_to plan[:company],meetings_path(anchor:plan[:id]) ,class: "meeting-show-path" %>
            </div>
            </li>
          <% end %>
        <% end %>
        </ul>
      </div>